// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}
model Blog{
  blogId    Int @id @default(autoincrement())
  createdAt DateTime
  title     String
  subtitle  String
  body      String
  author    String
}